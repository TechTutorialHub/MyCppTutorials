#include <iostream> 
using namespace std; 
int main() { 
    int size; 
    cout << "Enter the size of the multiplication table: "; 
    cin >> size; 
    cout << "Multiplication Table:" << endl; 
    for (int i = 1; i <= size; i++) { 
        for (int j = 1; j <= size; j++) { 
            cout << i * j << "\t"; 
        } 
        cout << endl; 
    } 
   return 0; 

} 
